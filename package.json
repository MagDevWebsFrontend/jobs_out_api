{
  "name": "jobs_out_api",
  "version": "1.0.0",
  "description": "API para plataforma de b√∫squeda de trabajos en Cuba",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "seed": "node src/seeders/index.js",
    "lint": "eslint src",
    "format": "prettier --write src/**/*.js",
    "swagger": "node swagger.js",
    "setup:db": "node src/utils/setupDatabase.js",
    "db:reset": "npm run seed",
    "db:test": "node test-db.js",
    "init": "node init-project.js",
    "db:sync": "node -e \"require('dotenv').config(); const { syncDatabase } = require('./src/models'); syncDatabase({ alter: true });\""
  },
  "keywords": [
    "jobs",
    "cuba",
    "api",
    "express",
    "postgresql",
    "sequelize"
  ],
  "author": "Equipo Jobs Out",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.3.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.19.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "sequelize-cli": "^6.6.3",
    "supertest": "^6.3.4"
  }
}
